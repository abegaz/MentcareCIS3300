		City = city;
		State = state;
		Age = age;
		Height = height;
		Weight = weight;
		ArrivalDate = arrivalDate;
		ExpectedOuttakeDate = expectedOuttakeDate;
		Treatment = treatment;
		DoctorID = doctorID;
		MedicinePrescribed = medicinePrescribed;
		MeetingLocation = meetingLocation;
	}
